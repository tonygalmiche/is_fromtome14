   <template id="is_external_layout_boxed" inherit_id="web.external_layout_boxed">
        <div class="o_boxed_header" position="replace">
            <div class="o_boxed_header">
                <div class="row mb8">
                    <div class="col-6">
                        <!-- <img t-if="res_company.logo" t-att-src="image_data_uri(res_company.logo)" alt="Logo"/> -->
                        <img t-if="o.partner_id.is_enseigne_id.name.image_1920" t-att-src="image_data_uri(o.partner_id.is_enseigne_id.name.image_1920)" alt="Logo"/>
                        
                    </div>
                    <div class="col-6 text-right mb4">
                        <h4 class="mt0" t-field="res_company.report_header"/>
                        <div name="res_company" class="mb4">
                            <!-- <h5 style="font-weight:700 !important;" t-field="res_company.name"/> -->
                            <h5 style="font-weight:700 !important;" t-field="o.partner_id.is_enseigne_id.name.name"/>
                            <br/>
                            <span t-field="res_company.street"/>
                            <br/>
                            <span t-field="res_company.zip"/>
                            <![CDATA[
                            &nbsp;
                            ]]>
                            <span t-field="res_company.city"/>
                            <br/>
                            <span t-field="res_company.country_id"/>
                            <div t-if="res_company.is_gln">
                                GLN:<span t-field="res_company.is_gln"/>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="text-center"  position="replace">
            <div class="text-center" style="font-size:10pt">
                <div t-field="company.report_footer"/>
                <div t-if="report_type == 'pdf'">
                    Page: <span class="page"/> / <span class="topage"/>
                </div>
            </div>
        </div>


    </template>


